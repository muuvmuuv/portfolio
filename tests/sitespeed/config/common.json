{
  "outputFolder": "./reports/latest",
  "cpu": true,
  "gzipHAR": true,
  "browsertime": {
    "iterations": 1,
    "browser": "chrome",
    "video": false,
    "headless": false,
    "screenshotParams": {
      "type": "jpg"
    }
  },
  "crawler": {
    "depth": 1
  },
  "grafana": {
    "host": "localhost",
    "port": "3000"
  },
  "graphite": {
    "host": "localhost",
    "port": "2003",
    "auth": "guest:guest",
    "webPort": "8080"
  },
  "axe": {
    "enable": true
  },
  "lighthouse": {
    "iterations": 1,
    "extends": "lighthouse:full",
    "puppeteer": {
      "headless": false
    },
    "settings": {
      "throttlingMethod": "simulate"
    }
  }
}