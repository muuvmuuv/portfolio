{
  "name": "marvin-digital",
  "version": "3.0.0",
  "private": true,
  "description": "Portfolio of Marvin Heilemann (@muuvmuuv)",
  "repository": {
    "type": "git",
    "url": "https://github.com/muuvmuuv/portfolio"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "rebuild": "run-s clean build",
    "compose": "npm run build && make up",
    "clean": "del-cli ./public/** ./.cache",
    "−−−−−−−−−−−−−−−−−−−−  DEPLOY  −−−−−−−−−−−−−−−−–−−−−−−": "",
    "deploy": "run-s rebuild lint test now",
    "now": "run-s now-deploy now-alias-version now-notify",
    "now-build": "npm run rebuild",
    "now-deploy": "now --local-config now.json",
    "now-alias-version": "now alias portfolio.muuvmuuv.now.sh $(echo v${npm_package_version} | sed 's/\\(.*\\)\\.[0-9]/\\1/;s/\\.//g')0.portfolio.muuvmuuv.now.sh",
    "now-notify": "node scripts/notify.js",
    "−−−−−−−−−−−−−−−−  AUDITS/ANALYTICS  −−−−−−−−−−−−−−−−–": "",
    "audit": "run-s build lighthouse audit-open",
    "audit-open": "run-p audit:*",
    "audit:report-mobile": "sh ./scripts/open.sh lhr.mobile.html",
    "audit:report-desktop": "sh ./scripts/open.sh lhr.desktop.html",
    "audit:treemap": "sh ./scripts/open.sh treemap.html",
    "lighthouse": "node ./scripts/lighthouse.js",
    "−−−−−−−−−−−−−−−−−−−  LINT/TEST  −−−−−−−−−−−−−−−−−−−−–": "",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint src/**/*.{js,jsx}",
    "−−−−−−−−−−−−−−−−−  SCRIPTS/TOOLS  −−−−−−−−−−−−−−−−–−−": "",
    "version": "node ./scripts/version.js",
    "−−−−−−−−−−−−−−−−−−−−  COMMON  −−−−−−−−−−−−−−−−–−−−−−−": "",
    "pretty": "pretty-quick"
  },
  "dependencies": {
    "@ibm/plex": "^2.0.0",
    "dayjs": "^1.8.16",
    "dotenv": "^8.1.0",
    "gatsby": "^2.15.28",
    "gatsby-image": "^2.2.23",
    "gatsby-plugin-canonical-urls": "^2.1.10",
    "gatsby-plugin-catch-links": "^2.1.12",
    "gatsby-plugin-humans-txt": "^1.1.4",
    "gatsby-plugin-layout": "^1.1.10",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-postcss": "^2.1.10",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.10",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.17",
    "gatsby-plugin-sharp": "^2.2.27",
    "gatsby-plugin-sitemap": "^2.2.16",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-images": "^3.1.25",
    "gatsby-remark-prismjs": "^3.3.16",
    "gatsby-source-filesystem": "^2.1.28",
    "gatsby-transformer-json": "^2.2.11",
    "gatsby-transformer-remark": "^2.6.26",
    "gatsby-transformer-sharp": "^2.2.19",
    "kleur": "^3.0.3",
    "node-sass": "^4.12.0",
    "postcss-easing-gradients": "^3.0.1",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-helmet": "^5.2.1",
    "react-helmet-async": "^1.0.3",
    "unist-util-visit": "^2.0.0",
    "vanilla-click-outside": "^1.3.0"
  },
  "devDependencies": {
    "del-cli": "^3.0.0",
    "eslint": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "lighthouse": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prompts": "^2.2.1",
    "semver": "^6.3.0",
    "taskz": "^1.3.0"
  }
}
