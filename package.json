{
  "name": "marvin-digital",
  "version": "3.15.0",
  "private": true,
  "description": "Portfolio of and by Marvin Heilemann (@muuvmuuv)",
  "repository": "git@github.com:muuvmuuv/portfolio.git",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "cross-env SITE_URL=\"https://marvin.lcl\" gatsby build",
    "rebuild": "run-s clean build",
    "clean": "gatsby clean",
    "serve": "now dev --listen 59235",
    "−−−−−−−−−−−−−−−−  AUDITS/ANALYTICS  −−−−−−−−−−−−−−−−–": "",
    "audit": "cross-env AUDIT=true run-s rebuild audit-server lighthouse audit-open",
    "audit-server": "make restart",
    "audit-open": "run-p audit:*",
    "audit-open:mobile": "node ./scripts/open-report.js lhr.mobile.html",
    "audit-open:desktop": "node ./scripts/open-report.js lhr.desktop.html",
    "audit-open:treemap": "node ./scripts/open-report.js treemap.html",
    "lighthouse": "node ./tests/lighthouse/index.js",
    "−−−−−−−−−−−−−−−−−−−  LINT/TEST  −−−−−−−−−−−−−−−−−−−−–": "",
    "pretest": "npm run rebuild && make restart",
    "test": "run-s test:*",
    "test:gatsby": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "test:sitespeed": "echo \"Test sitespeed with sitespeed.io\"",
    "lint": "run-s lint:*",
    "lint:js": "eslint ./gatsby ./plugins ./scripts ./src ./tests ./utils ./*.js",
    "lint:scss": "stylelint ./src/styles",
    "−−−−−−−−−−−−−−−−−  SCRIPTS/TOOLS  −−−−−−−−−−−−−−−−–−−": "",
    "changelog": "auto-changelog --package --commit-limit false",
    "version": "node ./scripts/version.js",
    "pretty": "pretty-quick"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "@react-spring/web": "^9.0.0-beta.34",
    "copy-text-to-clipboard": "^2.1.1",
    "dayjs": "^1.8.19",
    "dot-prop": "^5.2.0",
    "gatsby": "^2.19.10",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-breadcrumb": "^7.0.0",
    "gatsby-plugin-canonical-urls": "^2.1.19",
    "gatsby-plugin-catch-links": "^2.1.24",
    "gatsby-plugin-humans-txt": "^1.1.4",
    "gatsby-plugin-layout": "^1.1.21",
    "gatsby-plugin-manifest": "^2.2.40",
    "gatsby-plugin-mdx": "^1.0.69",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-offline": "^3.0.33",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-preact": "^3.1.25",
    "gatsby-plugin-react-axe": "^0.3.0",
    "gatsby-plugin-react-helmet-async": "^1.0.15",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-generator": "^1.0.4",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.20",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.4.4",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.43",
    "gatsby-source-filesystem": "^2.1.47",
    "gatsby-source-graphql": "^2.1.32",
    "gatsby-transformer-json": "^2.2.25",
    "gatsby-transformer-remark": "^2.6.49",
    "gatsby-transformer-sharp": "^2.3.13",
    "ionicons": "^4.6.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "normalize-url": "^5.0.0",
    "preact": "^10.2.1",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-helmet-async": "^1.0.4",
    "unist-util-visit": "^2.0.1",
    "vanilla-click-outside": "^2.0.0"
  },
  "devDependencies": {
    "auto-changelog": "^1.16.2",
    "chrome-launcher": "^0.12.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.6.8",
    "eslint-plugin-prettier": "^3.1.2",
    "graphql-type-json": "^0.3.1",
    "kleur": "^3.0.3",
    "lighthouse": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prompts": "^2.3.0",
    "semver": "^7.1.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-property-sort-order-smacss": "^5.2.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "taskz": "^1.3.0"
  }
}
