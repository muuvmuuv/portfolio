{
  "name": "marvin-digital",
  "version": "3.6.0",
  "private": true,
  "description": "Portfolio of and by Marvin Heilemann (@muuvmuuv)",
  "repository": {
    "type": "git",
    "url": "https://github.com/muuvmuuv/portfolio"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "rebuild": "run-s clean build",
    "clean": "gatsby clean",
    "serve": "now dev",
    "−−−−−−−−−−−−−−−−−−−−  DEPLOY  −−−−−−−−−−−−−−−−–−−−−−−": "",
    "deploy": "now --local-config now.json",
    "deploy-prod": "run-s lint rebuild test now-deploy-prod",
    "now-deploy-prod": "now --local-config now.json --prod",
    "−−−−−−−−−−−−−−−−  AUDITS/ANALYTICS  −−−−−−−−−−−−−−−−–": "",
    "audit": "run-s rebuild lighthouse audit-open",
    "audit-open": "run-p audit:*",
    "audit:report-mobile": "sh ./scripts/open.sh lhr.mobile.html",
    "audit:report-desktop": "sh ./scripts/open.sh lhr.desktop.html",
    "audit:treemap": "sh ./scripts/open.sh treemap.html",
    "lighthouse": "node ./scripts/lighthouse.js",
    "−−−−−−−−−−−−−−−−−−−  LINT/TEST  −−−−−−−−−−−−−−−−−−−−–": "",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint .",
    "−−−−−−−−−−−−−−−−−  SCRIPTS/TOOLS  −−−−−−−−−−−−−−−−–−−": "",
    "version": "node ./scripts/version.js",
    "pretty": "pretty-quick"
  },
  "dependencies": {
    "@ibm/plex": "^4.0.2",
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "dayjs": "^1.8.19",
    "gatsby": "^2.18.25",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-breadcrumb": "^6.3.0",
    "gatsby-plugin-canonical-urls": "^2.1.19",
    "gatsby-plugin-catch-links": "^2.1.24",
    "gatsby-plugin-humans-txt": "^1.1.4",
    "gatsby-plugin-layout": "^1.1.21",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-mdx": "^1.0.67",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-postcss": "^2.1.19",
    "gatsby-plugin-preact": "^3.1.25",
    "gatsby-plugin-react-axe": "^0.3.0",
    "gatsby-plugin-react-helmet-async": "^1.0.14",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-generator": "^1.0.4",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.20",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-emoji": "0.0.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-json": "^2.2.25",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.13",
    "ionicons": "^4.6.3",
    "kleur": "^3.0.3",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "normalize-url": "^4.5.0",
    "postcss-easing-gradients": "^3.0.1",
    "preact": "^10.2.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-helmet-async": "^1.0.4",
    "unist-util-visit": "^2.0.1",
    "vanilla-click-outside": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.6.5",
    "eslint-plugin-prettier": "^3.1.2",
    "lighthouse": "^5.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "prompts": "^2.3.0",
    "semver": "^7.1.1",
    "taskz": "^1.3.0"
  }
}
