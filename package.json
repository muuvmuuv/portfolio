{
  "name": "marvin-digital",
  "version": "5.4.0",
  "private": true,
  "description": "Portfolio of and by Marvin Heilemann (@muuvmuuv)",
  "repository": "git@github.com:muuvmuuv/portfolio.git",
  "license": "UNLICENSED",
  "scripts": {
    "start": "pnpm run develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "rebuild": "run-s clean build",
    "clean": "gatsby clean",
    "serve": "now dev --listen 59235",
    "−−−−−−−−−−−−−−−−–−−  AUDITS/ANALYTICS  −−−−−−−−−−−−−−−−–": "",
    "audit": "cross-env SITE_URL=\"https://marvin.lcl\" AUDIT=true run-s build audit-server lighthouse audit-open",
    "audit-server": "make restart",
    "audit-open": "run-p audit-open:*",
    "audit-open:mobile": "node scripts/open-report.js lhr.mobile.html",
    "audit-open:desktop": "node scripts/open-report.js lhr.desktop.html",
    "audit-open:treemap": "node scripts/open-report.js treemap.html",
    "audit-open:stats": "node scripts/open-report.js bundle-stats.html",
    "lighthouse": "node tests/lighthouse/index.js",
    "−−−−−−−−−−−−−−−−–−---−  LINT/TEST  −−−−−−−−−−−−−−−−−−−−–": "",
    "pretest": "pnpm run build && make restart",
    "test": "run-s test:*",
    "test:gatsby": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "test:sitespeed": "echo \"Test sitespeed with sitespeed.io\"",
    "lint": "run-p lint:*",
    "lint:js-source": "eslint --cache --cache-location src/.eslintcache --config src/.eslintrc.json --ext js,jsx,mdx src",
    "lint:js-engine": "eslint --cache --ext js,jsx .",
    "lint:scss": "stylelint --cache src/**/*.{scss,css}",
    "lint:commit": "commitlint --from HEAD~1",
    "−-−−−−−−−−−−−−−−−–−----−  TOOLS  −−−−−−----−−−−−−−−−−–−−": "",
    "release": "env-cmd -f .env.build release-it",
    "pretty": "pretty-quick"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "@reach/router": "^1.3.3",
    "@react-spring/web": "^9.0.0-beta.34",
    "autoprefixer": "^9.7.4",
    "copy-text-to-clipboard": "^2.1.1",
    "cssnano": "^4.1.10",
    "dayjs": "^1.8.21",
    "dot-prop": "^5.2.0",
    "gatsby": "^2.19.28",
    "gatsby-image": "^2.2.42",
    "gatsby-plugin-breadcrumb": "^9.0.1",
    "gatsby-plugin-bundle-stats": "^2.0.0",
    "gatsby-plugin-canonical-urls": "^2.1.20",
    "gatsby-plugin-catch-links": "^2.1.26",
    "gatsby-plugin-humans-txt": "^1.1.4",
    "gatsby-plugin-layout": "^1.1.22",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-mdx": "^1.0.75",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-pnpm": "^1.2.3",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-preact": "^3.1.26",
    "gatsby-plugin-react-helmet-async": "^1.0.15",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-generator": "^1.0.4",
    "gatsby-plugin-remove-trailing-slashes": "^2.1.21",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sass": "^2.1.29",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-simple-analytics": "^1.0.3",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-remark-autolink-headers": "^2.1.24",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.1.37",
    "gatsby-remark-embedder": "^1.12.0",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-source-graphql": "2.1.33",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-leasot": "^1.2.2",
    "gatsby-transformer-sharp": "^2.3.16",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "preact": "^10.3.3",
    "prism-react-renderer": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-axe": "^3.4.1",
    "react-dom": "^16.13.0",
    "react-helmet-async": "^1.0.4",
    "react-icons": "^3.9.0",
